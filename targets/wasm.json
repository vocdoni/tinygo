{
	"llvm-target":   "wasm32-unknown-wasi",
	"cpu":           "generic",
	"features":      "+bulk-memory,+nontrapping-fptoint,+sign-ext",
	"build-tags":    ["tinygo.wasm"],
	"goos":          "js",
	"goarch":        "wasm",
	"linker":        "wasm-ld",
	"libc":          "wasi-libc",
	"scheduler":     "asyncify",
	"default-stack-size": 16384,
	"cflags": [
		"-mbulk-memory",
		"-mnontrapping-fptoint",
		"-msign-ext"
	],
	"ldflags": [
		"--allow-undefined-file={root}/targets/wasm-undefined.txt",
		"--no-demangle",
		"--stack-first",
		"{root}/lib/libclang_rt.builtins-wasm32.a",
		"--initial-memory=2147483648",
	        "--max-memory=4294967296",
        	"-zstack-size=16384"
	],
	"extra-files": [
		"src/runtime/asm_tinygowasm.S"
	],
	"emulator":      "node {root}/targets/wasm_exec.js {}",
	"wasm-abi":      "js"
}
